{
  "name": "curso-de-webpack",
  "version": "1.0.0",
  "description": "primer curso de webpack",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack index.js --output bundle.js --mode development",
    "build:local": "webpack",
    "build:external": "webpack --config ./external/webpack.config.js",
    "build:css": "webpack --config ./css-style-loader/webpack.config.js",
    "build:extractcss": "webpack --config ./plugin-extract-text/webpack.config.js",
    "build:multi": "webpack --config ./multiples-entry-points/webpack.config.js",
    "build:dev": "webpack-dev-server --config ./webpack-dev-server/webpack.config.js",
    "build:babel": "webpack-dev-server --config ./babel-loader/webpack.config.js"
  },
  "author": "Ernesto",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/preset-es2016": "^7.0.0-beta.53",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "extract-text-webpack-plugin": "^4.0.0-beta.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hermesto/cursoWebpack.git"
  },
  "bugs": {
    "url": "https://github.com/hermesto/cursoWebpack/issues"
  },
  "homepage": "https://github.com/hermesto/cursoWebpack#readme"
}
